{
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",

  "eslint.run": "onSave",
  "eslint.validate": ["vue", "js"],
  "eslint.packageManager": "yarn",
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,

  "yaml.customTags": [
    "!Equals sequence",
    "!FindInMap sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Join sequence",
    "!Ref",
    "!Select sequence",
    "!Split sequence",
    "!Sub"
  ]
}
